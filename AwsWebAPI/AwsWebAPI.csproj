    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>c31c0f5d-1ed9-4a26-87f0-0c504fcdd96f</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
